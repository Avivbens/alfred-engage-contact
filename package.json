{
    "name": "alfred-open-whatsapp",
    "version": "1.2.1",
    "description": "Alfred Workflow to search contacts and open WhatsApp",
    "main": "dist/index.js",
    "type": "module",
    "scripts": {
        "prebuild": "rm -rf ./dist",
        "build": "tsc -p tsconfig.json",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "lint": "eslint \"{src,test}/**/*.ts\"",
        "lint:fix": "eslint \"{src,test}/**/*.ts\" --fix",
        "start:dev": "node dist/index.js --inspect",
        "test": "jest",
        "postinstall": "alfy-init",
        "preuninstall": "alfy-cleanup",
        "prepublish": "npm run build",
        "pre-commit": "pretty-quick --staged",
        "prepare": "husky install || exit 0"
    },
    "author": {
        "name": "Aviv Ben Shahar",
        "email": "avivbens87@gmail.com"
    },
    "keywords": [
        "alfred",
        "workflow",
        "whatsapp",
        "contacts",
        "search"
    ],
    "license": "MIT",
    "homepage": "https://github.com/Avivbens/alfred-open-whatsapp",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Avivbens/alfred-open-whatsapp.git"
    },
    "publishConfig": {
        "registry": "https://registry.npmjs.org/"
    },
    "maintainers": [
        {
            "name": "Aviv Ben Shahar",
            "email": "avivbens87@gmail.com"
        }
    ],
    "dependencies": {
        "alfy": "^1.1.0",
        "libphonenumber-js": "^1.10.28",
        "node-mac-contacts": "^1.6.1"
    },
    "devDependencies": {
        "@commitlint/cli": "^18.4.3",
        "@commitlint/config-conventional": "^18.4.3",
        "@semantic-release/changelog": "^6.0.3",
        "@semantic-release/commit-analyzer": "^9.0.2",
        "@semantic-release/git": "^10.0.1",
        "@semantic-release/github": "^8.1.0",
        "@semantic-release/npm": "^9.0.2",
        "@semantic-release/release-notes-generator": "^10.0.3",
        "@total-typescript/ts-reset": "^0.4.2",
        "@types/node": "^18.11.18",
        "@typescript-eslint/eslint-plugin": "^5.0.0",
        "@typescript-eslint/parser": "^5.0.0",
        "eslint": "^8.0.1",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-jest": "^27.6.3",
        "eslint-plugin-prettier": "^4.0.0",
        "husky": "^8.0.3",
        "prettier": "^2.8.3",
        "pretty-quick": "^3.1.3",
        "semantic-release": "^19.0.5",
        "ts-loader": "^9.2.3",
        "ts-node": "^10.9.2",
        "tsconfig-paths": "^3.10.1",
        "typescript": "^5.2.2"
    }
}
